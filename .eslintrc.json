{
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true,
    "webextensions": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": "off"
  }
}
